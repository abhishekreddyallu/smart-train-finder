{
  "name": "smart-train-finder",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_NAVITIA_TOKEN": "@navitia-token"
  },
  "build": {
    "env": {
      "REACT_APP_NAVITIA_TOKEN": "@navitia-token"
    }
  }
}
