# Smart Train Finder Environment Variables

# Navitia.io API Token
# Get your token from https://navitia.io/
REACT_APP_NAVITIA_TOKEN=your_navitia_token_here

# API Configuration
REACT_APP_API_BASE_URL=https://api.navitia.io/v1

# Application Configuration
REACT_APP_APP_NAME=Smart Train Finder
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_CACHING=true
REACT_APP_ENABLE_ANALYTICS=false

# Development Settings
REACT_APP_DEBUG_MODE=false
